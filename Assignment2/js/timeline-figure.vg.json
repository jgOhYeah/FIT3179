{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "data": {
    "url": "data/timeline.csv"
  },
  "width": 100,
  "height": 700,
  "layer": [
    {
      "mark": "line",
      "encoding": {
        "y": {
          "field": "date",
          "type": "temporal",
          "title": null,
          "sort": "descending"
        }
      }
    },
    {
      "mark": "point",
      "encoding": {
        "y": {
          "field": "date",
          "type": "temporal",
          "title": null
        }
      }
    },
    {
      "mark": "text",
      "encoding": {
        "y": {
          "field": "date",
          "type": "temporal",
          "title": null
        },
        "text": {
          "field": "title"
        }
      }
    }
  ]
}